<?xml version="1.0" encoding="UTF-8"?>
<qti-assessment-item xmlns="http://www.imsglobal.org/xsd/imsqtiasi_v3p0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.imsglobal.org/xsd/imsqtiasi_v3p0 https://purl.imsglobal.org/spec/qti/v3p0/schema/xsd/imsqti_asiv3p0_v1p0.xsd" identifier="order-mountains" title="Scottish Mountains" adaptive="false" time-dependent="false">
    <qti-response-declaration identifier="RESPONSE" cardinality="ordered" base-type="identifier">
        <qti-correct-response>
            <qti-value>BenNevis</qti-value>
            <qti-value>BenMacdui</qti-value>
            <qti-value>Bidean</qti-value>
            <qti-value>CreagMeagaidh</qti-value>
        </qti-correct-response>
    </qti-response-declaration>
    <qti-outcome-declaration identifier="SCORE" cardinality="single" base-type="float"/>
    <qti-outcome-declaration identifier="iSCORE" cardinality="single" base-type="integer"/>
    <qti-outcome-declaration identifier="FEEDBACK" cardinality="single" base-type="identifier"/>
    <qti-stylesheet href="flickrMash.css" type="text/css"/>
    <qti-item-body>
        <qti-order-interaction response-identifier="RESPONSE" shuffle="true" orientation="vertical">
            <qti-prompt>Put these mountains in <strong>DESCENDING</strong> order of height</qti-prompt>
            <qti-simple-choice identifier="BenNevis">
